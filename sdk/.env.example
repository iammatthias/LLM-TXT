# SDK Test Configuration
# Note: These can also be set in the root .env file

# Private key for test wallet (must have USDC on Base mainnet or testnet)
# Format: 0x followed by 64 hex characters
TEST_WALLET_PRIVATE_KEY=0x...

# API URL for integration tests
TEST_API_URL=https://api.llm-txt.fun

# Local API URL for development testing
TEST_LOCAL_API_URL=http://localhost:3000

# Skip integration tests (set to "true" to run only unit tests)
SKIP_INTEGRATION_TESTS=false
